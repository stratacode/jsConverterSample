#!/bin/sh
# This file is generated by the MainSettings annotation on class sc.layer.LayeredSystem - warning changes made here will be lost
CMDPATH="`type -p $0`"
DIRNAME=`dirname $CMDPATH`
java -mx2048m  -jar "$DIRNAME/sc.jar" -restartArgsFile /tmp/restart$$.tmp $*
SC_EXIT_STATUS=$?
while expr $? = 33; do
   java -mx2048m  -jar "$DIRNAME/sc.jar" -restartArgsFile /tmp/restart$$.tmp -restart
SC_EXIT_STATUS=$?
done
exit $SC_EXIT_STATUS
